{
    "name": "rhincodon/gitter-api",
    "description": "PHP package for interaction with Gitter REST API.",
    "keywords": [
        "gitter",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/rhincodon/gitter-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Isaychenko",
            "email": "rhinodontypicus@gmail.com",
            "homepage": "http://laravelinfo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.6.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/support": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "vlucas/phpdotenv": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Rhinodontypicus\\GitterApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rhinodontypicus\\GitterApi\\Test\\": "tests"
        }
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
